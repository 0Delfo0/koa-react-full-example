{
  "name": "koa-react-full-example",
  "version": "0.0.4",
  "description": "Full example using Koa, React, Mongoose, Gulp, Mocha",
  "keywords": [
    "koa",
    "react",
    "mongoose",
    "mocha",
    "gulp",
    "mocha",
    "example"
  ],
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dozoisch/koa-react-full-example"
  },
  "scripts": {
    "start": "gulp dev",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --harmony --reporter spec ./test/test-*.js"
  },
  "author": "Hugo Dozois <hugo@dozoisch.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.11.9"
  },
  "dependencies": {
    "browserify": "4.1.x",
    "browserify-shim": "3.5.x",
    "co-views": "0.2.x",
    "gulp": "3.8.x",
    "gulp-less": "1.2.x",
    "gulp-nodemon": "1.0.x",
    "gulp-react": "0.3.x",
    "jade": "1.3.x",
    "koa": "0.8.x",
    "koa-compress": "1.0.x",
    "koa-error": "1.1.x",
    "koa-logger": "1.2.x",
    "koa-response-time": "1.0.x",
    "koa-router": "3.1.x",
    "koa-static": "1.4.x",
    "lodash": "2.4.x",
    "mongoose": "3.9.x",
    "react": "0.10.x",
    "superagent": "0.18.x",
    "vinyl-source-stream": "0.1.x"
  },
  "devDependencies": {
    "co": "3.0.x",
    "mocha": "1.20.x",
    "should": "4.0.x",
    "supertest": "0.13.x"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
